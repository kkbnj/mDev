  .iconList
    p.iconList__title .iconList
    ul.iconList__list
      li.iconList__item
        a.iconList__itemA(href='' target='_blank')
          img.iconList__itemImg(src=IMAGE_DIR+'fb.svg' alt='Facebook')
      li.iconList__item
        a.iconList__itemA(href='' target='_blank')
          img.iconList__itemImg(src=IMAGE_DIR+'tw.svg' alt='X')
      li.iconList__item
        a.iconList__itemA(href='' target='_blank')
          img.iconList__itemImg(src=IMAGE_DIR+'insta.svg' alt='Instagram')
      li.iconList__item
        a.iconList__itemA(href='' target='_blank')
          img.iconList__itemImg(src=IMAGE_DIR+'line.svg' alt='LINE')

  .button
    a.button__a(href='' target='_blank')
      span.button__txt .button

  .button.button--arrowTop
    a.button__a(href='' target='_blank')
      span.button__txt .button

  .button.button--arrowBottom
    a.button__a(href='' target='_blank')
      span.button__txt .button

  .button.button--arrowLeft
    a.button__a(href='' target='_blank')
      span.button__txt .button

  .button.button--arrowRight
    a.button__a(href='' target='_blank')
      span.button__txt .button

