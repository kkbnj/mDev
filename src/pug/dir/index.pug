    li.creditItem
      p.creditItem__cont
        a.creditItem__a(href='' target='_blank')
          span.creditItem__txt T-SHIRTS / STUDIO NICHOLSON
          span.creditItem__txt ¥9,800(inc. tax) / 3458193413
          span.creditItem__buy BUY

    li.creditItem
